# Server Configuration
NODE_ENV=development
PORT=3001
HOST=0.0.0.0

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/movenow_admin?schema=public

# JWT Authentication
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=24h

# Kafka/Event Bus
KAFKA_BROKERS=localhost:9092
KAFKA_CLIENT_ID=admin-management-service
KAFKA_GROUP_ID=admin-management-consumer-group

# Admin-Specific Settings
SUPER_ADMIN_EMAIL=admin@movenow.com
DEFAULT_ADMIN_PASSWORD=changeme

# Feature Toggles
ENABLE_ANALYTICS=true
ENABLE_AUDIT_LOGS=true

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
