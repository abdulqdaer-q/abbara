# Example Kubernetes Secret
# DO NOT commit actual secrets to version control
# Create actual secret with: kubectl create secret generic pricing-secrets --from-literal=...

apiVersion: v1
kind: Secret
metadata:
  name: pricing-secrets
  labels:
    app: pricing-service
type: Opaque
stringData:
  database-url: "postgresql://user:password@postgres-host:5432/movenow_pricing"
  redis-url: "redis://redis-host:6379"
  google-maps-api-key: "your-google-maps-api-key-here"
  jwt-secret: "your-jwt-secret-here"
  admin-api-key: "your-admin-api-key-here"
